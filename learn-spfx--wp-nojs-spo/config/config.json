{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "no-js-web-part-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/noJsWebPart/NoJsWebPartWebPart.js",
          "manifest": "./src/webparts/noJsWebPart/NoJsWebPartWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-3.6.0.min.js",
      "globalName": "jquery"
    },
    "bootstrap": {
      "path": "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js",
      "globalName": "bootstrap",
      "globalDependencies": ["jquery"]
    }
  },
  "localizedResources": {
    "NoJsWebPartWebPartStrings": "lib/webparts/noJsWebPart/loc/{locale}.js"
  }
}
